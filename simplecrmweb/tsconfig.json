{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "types": ["cypress","node"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports":true,
    "esModuleInterop":true

  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}